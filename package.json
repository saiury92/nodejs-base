{
  "name": "qlear-nft-be",
  "version": "1.0.0",
  "description": "QlearNFT Backend",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon serve.js",
    "db1:create": "npx sequelize-cli --options-path ./.sequelizerc-db1 db:create",
    "db2:create": "npx sequelize-cli --options-path ./.sequelizerc-db2 db:create",
    "db1:migrate:up": "npx sequelize-cli --options-path ./.sequelizerc-db1 db:migrate",
    "db2:migrate:up": "npx sequelize-cli --options-path ./.sequelizerc-db2 db:migrate",
    "db1:migrate:undo": "npx sequelize-cli --options-path ./.sequelizerc-db1 db:migrate:undo",
    "db2:migrate:undo": "npx sequelize-cli --options-path ./.sequelizerc-db2 db:migrate:undo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nft",
    "nodejs",
    "express",
    "sequelize",
    "mysql",
    "rest",
    "api"
  ],
  "author": "thien.nguyen1",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.5"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2"
  }
}
